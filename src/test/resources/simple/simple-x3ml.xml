                        </arg>
                        <arg name="entry">
                            <xpath>identifier/text()</xpath>
                        </arg>
                    </uri_function>
                </entity>
            </target>
        </domain>
        <link>
            <path>
                <source>
                    <xpath>type</xpath>
                </source>
                <target>
                    <property>
                        <qname>crm:P2_has_type</qname>
                    </property>
                </target>
            </path>
            <range>
                <source/>
                <target>
                    <entity>
                        <qname>crm:E55_Type</qname>
                        <uri_function name="Type"/>
                    </entity>
                </target>
            </range>
        </link>
    </mapping>
</mappings>