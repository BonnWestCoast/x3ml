<?xml version="1.0" encoding="UTF-8"?>
<mappings version="0.1">
    <namespaces>
        <namespace prefix="crm" uri="http://purl.org/NET/cidoc-crm/core#"/>
    </namespaces>
    <mapping>
        <domain>
            <source>
                <xpath>//movie</xpath>
            </source>
            <entity>
                <qname>crm:E19_Physical_Object</qname>
                <uri_function name="PhysicalObject">
                    <arg name="nameOfMuseum">
                        <xpath>museum/text()</xpath>
                    </arg>
                    <arg name="entry">
                        <xpath>identifier/text()</xpath>
                    </arg>
                </uri_function>
            </entity>
        </domain>
        <link>
            <path>
                <source>
                    <xpath>type</xpath>
                </source>
                <property>
                    <qname>crm:P2_has_type</qname>
                </property>
            </path>
            <range>
                <entity>
                    <qname>crm:E55_Type</qname>
                    <uri_function name="Type"/>
                </entity>
            </range>
        </link>
    </mapping>
</mappings>