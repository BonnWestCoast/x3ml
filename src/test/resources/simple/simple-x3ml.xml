<?xml version="1.0" encoding="UTF-8"?>
<mappings version="0.1">
    <namespaces>
        <namespace prefix="crm" uri="http://purl.org/NET/cidoc-crm/core#"/>
    </namespaces>
    <mapping>
        <domain>
            <source>
                <xpath>//movie</xpath>
            </source>
            <target>
                <entity>
                    <qname>crm:E19_Physical_Object</qname>
                    <uri_function name="PhysicalObject">
                        <arg name="nameOfMuseum">
