<?xml version="1.0" encoding="UTF-8"?>
<mappings version="1.0">
    <namespaces>
        <namespace prefix="crm" uri="http://www.cidoc-crm.org/cidoc-crm/"/>
        <namespace prefix="rdf" uri="http://www.w3.org/1999/02/22-rdf-syntax-ns#"/>
    </namespaces>
    <mapping>
        <domain>
            <source>
                <xpath>//COIN</xpath>
            </source>
            <entity>
                <qname>crm:E22_Man-Made_Object</qname>
                <uri_function name="UUID"/>
            </entity>
        </domain>
        <link>
            <path>
                <source>
                    <xpath>ID</xpath>
                </source>
                <property>
                    <qname>crm:P1_is_identified_by</qname>
                </property>
            </path>
            <range>
                <!--<source>-->
                    <!--<xpath>ID</xpath>-->
                <!--</source>-->
                <entity>
                    <qname>crm:E41_Appellation</qname>
                    <uri_function name="UUID"/>
                </entity>
                <additional_node>
                    <property>
                        <qname>crm:P1_is_identified_by</qname>
                    </property>
                    <entity>
                        <qname>rdf:label</qname>
                        <xpath>text()</xpath>
                    </entity>
                </additional_node>
            </range>
        </link>
    </mapping>
</mappings>